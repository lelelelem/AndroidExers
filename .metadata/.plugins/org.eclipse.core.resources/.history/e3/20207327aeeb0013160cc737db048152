import java.io.*;
import java.util.Scanner;


public class Chapter10Class extends Files {

	private BufferedReader FSOriginal;
	
	
	private FileWriter FSNew;
	
	
	Chapter10Class()throws IOException{
		
		FSOriginal = new BufferedReader(new FileReader(ReturnFileNameOriginal()));
		
		
		FSNew = new FileWriter(ReturnFileNameNew());
	
	}
	
	void Read() throws IOException{
		
		int i=0;
		
		for(;i!=-1;i=FSOriginal.read()){
		
			
			
			
			FSNew.write((char)i);
			
		}
		
		FSNew.close();
		FSOriginal.close();
		
	}
	
}
