
public class Chapter14Class {

	LinkedList head;
	
	
	Chapter14Class(){
		head = null;
	}
	
	void addhead(int x, int y){
		
		LinkedList NewNode = new LinkedList();
		NewNode.next = head;
		head = NewNode;
		
		head.x = x;
		head.y = y;
	}
	
	void traverse(){
		
		for(LinkedList temp = head; temp!=null;temp = temp.next){
			System.out.print(temp.x+","+temp.y);
			
		}
		
	}
	
}


class LinkedList{
	
	LinkedList head,next;
	int x, y;
	
}
