import java.lang.*;

public class Chapter11Class extends Thread {

	private Name;
	
	Chapter11Class(String Name){
		start();
	}
	
	public void run() {
		
		
		
			try {
				while(true){
				System.out.println("Running...");
				Thread.sleep(400);
				}
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
	
		
	}

}
