
import java.applet.Applet;
import java.util.*;

class Exercise {	
	
	  public static void main(String args[])
	    { 
	      B b = new B();
	      b.myMethod();
	      
	      double d = Math.round(2.5);
	      System.out.print(d);
	    }
	
	
	
}

class A{
	
	
	A(String s) {
		System.out.println("A");
	}

	void myMethod() {
		System.out.println("A my method");
	}
}

class B extends A{
	
	
	B() {
		super("");
		System.out.println("B");
	}

	void myMethod() {
		System.out.println("B my method");
	}
}
