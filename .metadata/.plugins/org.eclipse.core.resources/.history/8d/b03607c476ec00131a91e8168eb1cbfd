import java.awt.*;
import java.awt.geom.Line2D;


public class Chapter14Class {

	LinkedList head;
	
	
	Chapter14Class(){
		head = null;
	}
	
	void addhead(int PrevX, int PrevY,int x, int y){
		
		LinkedList NewNode = new LinkedList();
		NewNode.next = head;
		head = NewNode;
		
		head.x = x;
		head.y = y;
		head.PrevX = PrevX;
		head.PrevY = PrevY;
	}
	
	Line2D[] traverse(){
		
		for(LinkedList temp = head; temp!=null;temp = temp.next){
			System.out.print(temp.x+","+temp.y+"\n");
			
		}
		
	}
	
}


class LinkedList{
	
	LinkedList head,next;
	int x, y, PrevX, PrevY;
	
}
