import java.io.IOException;


public class Chapter3Class {

	void ChangeCases() throws IOException{
		
		//97-122 lowercase characters
		//65-90 uppercase characters
		
		for(char c=' ';c!='.';c= (char)System.in.read()){
			if (c>=97&&c<=122){
				
				System.out.print(c-32);
				
			}
			else if (c>=65&&c<=90){
				
				System.out.print(c-32);
				
			}
			
			
		}
		
		
		
	}
	
}
