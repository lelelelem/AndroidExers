
import java.applet.Applet;
import java.util.*;

class Exercise {	
	
	  public static void main(String args[])
	    { 
	      B b = new B();
	      b.myMethod();
	    }
	
	
	
}

class A{
	
	
	A() {
		System.out.println("A");
	}

	void myMethod() {
		System.out.println("A my method");
	}
}

class B{
	
	
	B() {
		System.out.println("B");
	}

	void myMethod() {
		System.out.println("B my method");
	}
}
